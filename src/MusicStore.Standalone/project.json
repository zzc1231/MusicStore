{
  "authors": [
    "Microsoft"
  ],
  "description": "Music store application on ASP.NET 5",
  "version": "1.0.2",
  "buildOptions": {
    "compile": {
      "exclude": [
        "../MusicStore/Program.cs",
        "../MusicStore/bin",
        "../MusicStore/obj"
      ],
      "include": [
        "../../shared/**/*.cs",
        "../MusicStore/**/*.cs"
      ]
    },
    "copyToOutput": {
      "include": [
        "Areas",
        "Views",
        "wwwroot",
        "config.json",
        "web.config"
      ]
    },
    "define": [
      "DEMO",
      "TESTING"
    ],
    "emitEntryPoint": true,
    "preserveCompilationContext": true,
    "warningsAsErrors": true
  },
  "publishOptions": {
    "include": [
      "Areas",
      "Views",
      "wwwroot",
      "config.json",
      "web.config"
    ]
  },
  "dependencies": {
    "Microsoft.AspNetCore.Authentication.Cookies": "1.0.2",
    "Microsoft.AspNetCore.Authentication.Facebook": "1.0.2",
    "Microsoft.AspNetCore.Authentication.Google": "1.0.2",
    "Microsoft.AspNetCore.Authentication.MicrosoftAccount": "1.0.2",
    "Microsoft.AspNetCore.Authentication.OpenIdConnect": "1.0.2",
    "Microsoft.AspNetCore.Authentication.Twitter": "1.0.2",
    "Microsoft.AspNetCore.Diagnostics": "1.0.2",
    "Microsoft.AspNetCore.Diagnostics.EntityFrameworkCore": "1.0.2",
    "Microsoft.AspNetCore.Identity.EntityFrameworkCore": "1.0.2",
    "Microsoft.AspNetCore.Mvc": "1.0.3",
    "Microsoft.AspNetCore.Mvc.TagHelpers": "1.0.3",
    "Microsoft.AspNetCore.Server.IISIntegration": "1.0.2",
    "Microsoft.AspNetCore.Server.Kestrel": "1.0.3",
    "Microsoft.AspNetCore.Server.WebListener": "1.0.2",
    "Microsoft.AspNetCore.Session": "1.0.2",
    "Microsoft.AspNetCore.StaticFiles": "1.0.2",
    "Microsoft.EntityFrameworkCore.InMemory": "1.0.3",
    "Microsoft.EntityFrameworkCore.SqlServer": "1.0.3",
    "Microsoft.Extensions.Configuration.CommandLine": "1.0.2",
    "Microsoft.Extensions.Configuration.EnvironmentVariables": "1.0.2",
    "Microsoft.Extensions.Configuration.Json": "1.0.2",
    "Microsoft.Extensions.Logging.Console": "1.0.2",
    "Microsoft.Extensions.Options.ConfigurationExtensions": "1.0.2"
  },
  "frameworks": {
    "netcoreapp1.0": {
      "dependencies": {
        "Microsoft.NETCore.App": {
          "version": "1.0.3"
        }
      }
    }
  },
  "tools": {
    "Microsoft.AspNetCore.Server.IISIntegration.Tools": "1.0.0-preview4-update1"
  },
  "scripts": {
    "postpublish": "dotnet publish-iis --publish-folder %publish:OutputPath% --framework %publish:FullTargetFramework%"
  },
  "runtimes": {
    "win7-x64": {},
    "win7-x86": {},
    "osx.10.10-x64": {},
    "osx.10.11-x64": {},
    "ubuntu.14.04-x64": {},
    "ubuntu.15.04-x64": {}
  }
}